{
  "name": "@triadxyz/firethree-protocol",
  "version": "0.0.4",
  "description": "The official Firethree. Create your own solana dApp and build on the best of socialFI products!",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && tsc",
    "prepublishOnly": "yarn build",
    "docs": "typedoc --excludePrivate --includeVersion --out ./../docs src/*",
    "start": "yarn build && node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/triadxyz/firethree-protocol"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "files": [
    "./dist/**/*"
  ],
  "keywords": [
    "solana",
    "blockchain",
    "protocol",
    "firethree",
    "web3 collection",
    "web3 storage",
    "web3 analytics",
    "GenesysGO shadow drive",
    "vaults"
  ],
  "author": "Triad",
  "license": "MIT",
  "homepage": "https://firethree.xyz",
  "dependencies": {
    "@coral-xyz/anchor": "0.28.0",
    "@solana/web3.js": "1.89.1",
    "@solana/spl-token": "^0.3.11",
    "@shadow-drive/sdk": "5.0.0",
    "axios": "^1.5.1",
    "bs58": "5.0.0",
    "uuid": "^9.0.1",
    "@pythnetwork/client": "^2.19.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "eslint": "8.50.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "prettier": "3.0.3",
    "typedoc": "0.25.1",
    "typescript": "5.2.2"
  }
}
